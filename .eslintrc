{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-useless-escape": "off",
    "linebreak-style": "off",
    "object-curly-spacing" : "off",
    "require-jsdoc" : "off",
    "no-trailing-spaces" : "off",
    "max-len": 0,
    "indent": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
